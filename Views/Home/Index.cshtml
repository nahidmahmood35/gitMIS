<div class="row">
    <div class="col-md-4">
        <h2 id="txtName">
            
        </h2>
    </div>
</div>

    <script>  
        $(document).ready(function () {
         $.ajax({  
             type: "GET",  
             url: "/api/Values/GetNameOfRakib",
             contentType: "application/json; charset=utf-8",  
             dataType: "json",  
             success: function (data) {
                 jQuery.each(data, function (i, val) {
                     $("#txtName").append(document.createTextNode(val + " - "));
                 });
             }, 
             failure: function (data) {  
                 alert(data.responseText);  
             }, 
             error: function (data) {  
                 alert(data.responseText);  
             } 
         });         
     });  
    </script>  
